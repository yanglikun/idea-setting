<keymap version="1" name="Default copy" parent="$default">
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="alt s" />
  </action>
  <action id="EditorPasteFromX11" />
  <action id="ExtractMethod">
    <keyboard-shortcut first-keystroke="e" second-keystroke="m" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt w" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="ctrl alt b" />
    <mouse-shortcut keystroke="control alt button1" />
    <mouse-shortcut keystroke="button2" />
  </action>
  <action id="IntroduceConstant">
    <keyboard-shortcut first-keystroke="e" second-keystroke="c" />
  </action>
  <action id="IntroduceField">
    <keyboard-shortcut first-keystroke="e" second-keystroke="f" />
  </action>
  <action id="IntroduceParameter">
    <keyboard-shortcut first-keystroke="e" second-keystroke="p" />
  </action>
  <action id="IntroduceVariable">
    <keyboard-shortcut first-keystroke="e" second-keystroke="v" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
</keymap>